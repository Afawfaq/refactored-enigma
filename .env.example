# Ubuntu 25.04 Environment Configuration
# Copy this to .env and customize for your system

# Display configuration
DISPLAY=:0
GDK_BACKEND=x11

# Ollama configuration (TCP - simplest setup)
OLLAMA_HOST=http://host.docker.internal:11434

# AMD GPU configuration (7800 XT / RDNA 3)
HSA_OVERRIDE_GFX_VERSION=11.0.0
ROCR_VISIBLE_DEVICES=0

# ROCm configuration
ROC_ENABLE_PRE_VEGA=0
HCC_AMDGPU_TARGET=gfx1101

# Logging
LOG_LEVEL=INFO

# Session defaults
DEFAULT_SESSION_DURATION=20
DEFAULT_PERSONA=gentle guide
